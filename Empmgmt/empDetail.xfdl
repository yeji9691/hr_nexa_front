this.fn_callback =function(trID ,ErrorCode ,ErrorMsg)
{
if(trID=="searcDeptID"){
			var empCode = this.ds_empList.getColumn(this.ds_empList.rowposition,"EMP_CODE");
			this.Div00_00.form.Tab00_00.Tabpage1.form.empImage.set_image("")
