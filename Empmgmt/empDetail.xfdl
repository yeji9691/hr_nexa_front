	}if(!this.confirm(" 변경한 내용을 서버에 저장하시겠습니까?  ")){
		alert("취소 ");
		return;
	}if(status!="insert" && status!="delete"){
		
		trace(this.ds_empList.getColumn(this.ds_empList.rowposition,"EMP_CODE"));
